<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:contextHubPath="/etc/cloudsettings/asset-share-commons/contexthub"
        cq:contextHubSegmentsPath="/etc/segmentation/contexthub"
        cq:template="/conf/asset-share-commons/test/settings/wcm/templates/search-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="asset-share-commons/components/structure/search-page">
        <config jcr:primaryType="nt:unstructured">
            <actions jcr:primaryType="nt:unstructured">
                <cart
                    jcr:primaryType="nt:unstructured"
                    enabled="true"/>
                <download
                    jcr:primaryType="nt:unstructured"
                    enabled="true"/>
                <license
                    jcr:primaryType="nt:unstructured"
                    enabled="{Boolean}true"/>
                <share
                    jcr:primaryType="nt:unstructured"
                    enabled="{Boolean}true"/>
            </actions>
            <asset-details
                jcr:primaryType="nt:unstructured"
                placeholderPath="/apps/asset-share-commons/resources/placeholder.jpeg"
                selector="always-use-default"/>
            <messages jcr:primaryType="nt:unstructured">
                <cart-add
                    jcr:primaryType="nt:unstructured"
                    eventId="cart-add"
                    style="success"
                    text="Asset added to cart"/>
                <cart-remove
                    jcr:primaryType="nt:unstructured"
                    eventId="cart-remove"
                    style="notice"
                    text="Asset removed from cart"/>
            </messages>
        </config>
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="asset-share-commons/components/structure/root">
            <rail
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <cq:responsive jcr:primaryType="nt:unstructured"/>
                <responsivegrid
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                </responsivegrid>
            </rail>
            <main
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <cq:responsive jcr:primaryType="nt:unstructured"/>
                <responsivegrid
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                </responsivegrid>
                <header
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="asset-share-commons/components/structure/header"/>
            </main>
        </root>
    </jcr:content>
</jcr:root>