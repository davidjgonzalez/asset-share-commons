{
  "name": "ui.tests",
  "scripts": {
    "setup": "npm install && node node_modules/.bin/selenium-standalone install",
    "selenium:start": "node_modules/.bin/selenium-standalone start",
    "test": "node node_modules/webdriverio/bin/wdio $npm_config_wdio_config",
    "test:localhost:publish": "node node_modules/webdriverio/bin/wdio ./test/config/wdio.localhost.macos.publish.conf.js",
    "report:mochawesome": "marge ./report/wdiomochawesome.json --inline --reportTitle 'Asset Share Commons ui.tests Results' --reportFilename 'asset-share-commons-results.html' --reportDir './report' && open report/asset-share-commons-results.html"
  },
  "config": {
    "wdioConfig": "./test/config/wdio.localhost.macos.publish.conf.js"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "fibers": "^3.1.1",
    "mocha": "^5.2.0",
    "mochawesome-report": "^1.0.0-alpha.1",
    "mochawesome-report-generator": "^3.1.5",
    "wdio-mochawesome-reporter": "^2.0.1",
    "webdriverio": "^4.14.2"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "deepmerge": "^2.2.1",
    "mochawesome": "^3.1.1",
    "selenium-standalone": "^6.15.4",
    "wdio-concise-reporter": "^0.1.2",
    "wdio-cucumber-framework": "^2.2.8",
    "wdio-json-reporter": "^0.3.1",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-selenium-standalone-service": "0.0.12",
    "wdio-spec-reporter": "^0.1.5"
  }
}
